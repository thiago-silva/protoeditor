<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kcfg SYSTEM "http://www.kde.org/standards/kcfg/1.0/kcfg.dtd">
<kcfg>
  <kcfgfile name="protoeditorrc"/>
  <group name="Browser">
    <entry name="UseExternalBrowser" type="Bool">
      <label>Use external browser</label>
      <default>false</default>
    </entry>
    <entry name="BrowserCmd" type="String">
      <label>Browser command</label>
      <default>konqueror</default>
    </entry>
  </group>
</kcfg>
